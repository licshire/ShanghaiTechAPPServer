    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-graphql": "^0.2.4",
    "eslint-plugin-import": "^1.14.0",
