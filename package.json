    "chinese-random-name": "^0.1.2",
    "color": "~0.11.1",
    "eslint-plugin-flowtype": "^2.6.1",
    "express": "^4.14.0",
    "graphql-tools": "^0.6.6",
    "is-uuid": "^1.0.2",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "neo4j-driver": "^1.1.0-M01",
    "node-fetch": "^1.6.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.10.0",
    "pokemon-random-name": "^1.0.4",
